{
    "name": "SAFE",
    "version": "0.5",
    "description": "Snulife Average Face Expectation",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://snulife.com/best/*",
                "https://snulife.com/index.php?mid=best*"
            ],
            "css": [
                "snulife.css"
            ]
        },
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "youtube.js"
            ],
            "css": [
                "youtube.css"
            ]
        },
        {
            "matches": [
                "*://www.ppomppu.co.kr/*"
            ],
            "css": [
                "ppomppu.css"
            ]
        },
        {
            "matches": [
                "https://v4.map.naver.com/*"
            ],
            "css": [
                "naver.map.css"
            ]
        },
        {
            "matches": [
                "https://news.naver.com/*"
            ],
            "css": [
                "naver.news.css"
            ]
        }
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}